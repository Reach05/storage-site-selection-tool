
{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "env": {
    "HEATMAP_SERVICE_URL": "...",
    "DRIVETIME_SERVICE_URL": "...",
    "FLOODZONE_SERVICE_URL": "..."
  },
  "rewrites": [
    { "source": "/api/:path*", "destination": "/api/:path*" }
  ]
}

